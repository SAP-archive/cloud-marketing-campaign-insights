<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout">
	<Table id="tableCampaignTeamMembers" growing="true" growingThreshold="20" busy="true" growingScrollToLoad="true">
		<headerToolbar>
			<Toolbar>
				<Title id="titleCampaignteamMember" text="{objectView>/campaignTeamMemberTableTitle}"></Title>
				<ToolbarSpacer/>
			</Toolbar>
		</headerToolbar>
		<columns>
			<Column minScreenWidth="Desktop" demandPopin="true">
				<Text text="{i18n>objectPage.contacts.name_new}"/>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true">
				<Text text="{i18n>objectPage.contacts.email_new}"/>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true">
				<Text text="{i18n>objectPage.contacts.phone_new}"/>
			</Column>
		</columns>
	</Table>
</core:FragmentDefinition>